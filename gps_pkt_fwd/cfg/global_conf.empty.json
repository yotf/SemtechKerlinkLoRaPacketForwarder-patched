		"chan_multiSF_7": {
			"enable": false,
			"radio": 0,
			"if": 0
		},
		
		/* the 'stand-alone' Lora modem can receive emissions with BW 125, 250 or 500 kHz, on a specific SF */
		"chan_Lora_std": {
			"enable": false,
			"radio": 0,
			"if": 0,
			"bandwidth": 250000,
			"spread_factor": 7
		},
		
		/* 'high speed' FSK channel, use for radio backhaul, or for maintenance, remote update, etc */
		"chan_FSK": {
			"enable": false,
			"radio": 0,
			"if": 100000,
			"bandwidth": 250000,
			"datarate": 100000
		}
	},
