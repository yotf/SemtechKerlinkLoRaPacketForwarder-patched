		/* 'multi' Lora channels car receive Lora packets with a BW of 125 kHz, and any SF is automatically recognized and demodulated */
		/* Note: chan_multiSF_3 to 7 are not available on the FPGA version */
		"chan_multiSF_0": {
			"enable": false,
			"radio": 0,
			"if": 0
